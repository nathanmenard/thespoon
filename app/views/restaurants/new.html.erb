<h1>New restaurant</h1>

<%= form_for(Restaurant.new) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.label :chef %>
  <%= f.text_field :chef %>
  <%= f.submit %>
<% end %>
